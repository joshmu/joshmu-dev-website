{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/layout/*": [
        "src/components/Layout/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/shared/*": [
        "src/components/shared/*"
      ],
      "@/context/*": [
        "src/context/*"
      ],
      "@/hooks/*": [
        "src/hooks/*"
      ],
      "@/pages/*": [
        "pages/*"
      ],
      "@/api/*": [
        "pages/api/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/services/*": [
        "src/services/*"
      ],
      "@/utils/*": [
        "utils/*"
      ]
    },
    "checkJs": true,
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
